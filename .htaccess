<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /zm-blog/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /zm-blog/index.php [L]
RewriteCond %{http_host} ^ihakula.com:9000/zm-blog/ [nc]
RewriteRule ^(.*)$ http://no1.mydomain.com/$1 [r=301,nc]
</IfModule>

